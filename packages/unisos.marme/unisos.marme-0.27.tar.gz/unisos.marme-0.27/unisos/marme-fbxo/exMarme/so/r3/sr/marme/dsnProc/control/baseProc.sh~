function vis_tarPrepsFull {
    G_funcEntry
    function describeF {  G_funcEntryShow; cat  << _EOF_
** Get rid of symlinks
- distClean
- symLinks remove
_EOF_
    }
    EH_assert [[ $# -eq 0 ]]

    local icmsPkgBaseDir=$( FN_absolutePathGet .. )
    local icmsPkgName=$( cat _icmsPkgName )

    opDo vis_pkgDistClean
    opDo vis_icmPanelsClean
    opDo vis_icmsPkgBaseLinksRemove
    opDo vis_icmsLibsLinksRemove    
    
    lpReturn
}


_CommentBegin_
*  [[elisp:(org-cycle)][| ]]  [[elisp:(blee:ppmm:org-mode-toggle)][Nat]] [[elisp:(beginning-of-buffer)][Top]] [[elisp:(delete-other-windows)][(1)]] || IIF       ::  vis_icmsPkgBaseLinks    [[elisp:(org-cycle)][| ]]
_CommentEnd_

function vis_icmsPkgBaseLinks {
    G_funcEntry
    function describeF {  G_funcEntryShow; cat  << _EOF_
** 
- distClean
- symLinks remove
_EOF_
    }
    EH_assert [[ $# -eq 0 ]]

    local icmsPkgBaseDir=$( FN_absolutePathGet .. )
    local icmsPkgName=$( cat _icmsPkgName )
    local icmsBxoBaseDir=$( FN_absolutePathGet ../../.. )  #

    if [ -h ${icmsPkgBaseDir}/var ] ; then
	if [ -h ${icmsPkgBaseDir}/tmp ] ; then
	    lpReturn 0
	fi
    fi

    opDoRet  mkdir ${icmsBxoBaseDir}/tmp
    opDoRet  mkdir ${icmsBxoBaseDir}/tmp/${icmsPkgName}        
    opDoRet  mkdir ${icmsBxoBaseDir}/var
    opDoRet  mkdir ${icmsBxoBaseDir}/var/${icmsPkgName}    

    opDo ln -s ${icmsBxoBaseDir}/tmp/${icmsPkgName} ${icmsPkgBaseDir}/tmp
    opDo ln -s ${icmsBxoBaseDir}/var/${icmsPkgName} ${icmsPkgBaseDir}/var    
    
    lpReturn
}


_CommentBegin_
*  [[elisp:(org-cycle)][| ]]  [[elisp:(blee:ppmm:org-mode-toggle)][Nat]] [[elisp:(beginning-of-buffer)][Top]] [[elisp:(delete-other-windows)][(1)]] || IIF       ::  vis_icmsPkgBaseLinksRemove    [[elisp:(org-cycle)][| ]]
_CommentEnd_

function vis_icmsPkgBaseLinksRemove {
    G_funcEntry
    function describeF {  G_funcEntryShow; cat  << _EOF_
** 
- distClean
- symLinks remove
_EOF_
    }
    EH_assert [[ $# -eq 0 ]]

    local icmsPkgBaseDir=$( FN_absolutePathGet .. )
    local icmsPkgName=$( cat _icmsPkgName )
    local icmsBxoBaseDir=$( FN_absolutePathGet ../../.. )  #

    opDo FN_fileSymlinkRemoveIfThere ${icmsPkgBaseDir}/var
    opDo FN_fileSymlinkRemoveIfThere ${icmsPkgBaseDir}/tmp
    
    lpReturn
}


_CommentBegin_
*  [[elisp:(org-cycle)][| ]]  [[elisp:(blee:ppmm:org-mode-toggle)][Nat]] [[elisp:(beginning-of-buffer)][Top]] [[elisp:(delete-other-windows)][(1)]] || IIF       ::  vis_icmsLibsLinks    [[elisp:(org-cycle)][| ]]
