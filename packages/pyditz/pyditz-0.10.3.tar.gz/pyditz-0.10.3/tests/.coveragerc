# Coverage config file.

[html]
title = PyDitz coverage report

[run]
source = ditz
branch = False

#omit = colorize term

[report]
exclude_lines =
    pragma: no cover

    def do_ipython
    def do_shell
    def do_quit

    def getcomment
    def getchoice
    def getyesno
    def getline
    def gettext

    def setup
    def default
    def precmd
    def emptyline
    def complete_
    def match_
    def load_plugin
    def .*terminal_size
    def get_userconfig
    def warn
    def .*editor

    def _invalid
    self._invalid

    unimplemented

    class VCS
    class Config

    except .*KeyboardInterrupt
    except ImportError
    except Exception

    raise NotImplementedError

    if self.interactive
    if userconfig
    if self.vcs
    if self.termlines
    = self.last_

    if __name__ == .__main__.

precision = 1
