    <cppa:ServiceSpecification>
        <cppa:PartyRole name="Buyer"/>
        <cppa:CounterPartyRole name="Seller"/>
        <cppa:ServiceBinding>
            <cppa:Description xml:lang="en">UBL-based ordering service</cppa:Description>
            <cppa:Service>OrderOnly</cppa:Service>
            <cppa:ActionBinding id="BII03_001_S" action="SubmitOrder" sendOrReceive="send" >
                <cppa:ChannelId>ch1</cppa:ChannelId>
                <cppa:PayloadProfileId>pp1</cppa:PayloadProfileId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        <cppa:ServiceBinding>
            <cppa:Description xml:lang="en">Another UBL-based ordering service</cppa:Description>
            <cppa:Service>Ordering</cppa:Service>
            <cppa:ActionBinding id="BII28_001_S" action="SubmitOrder" sendOrReceive="send" >
                <cppa:ChannelId>ch1</cppa:ChannelId>
                <cppa:PayloadProfileId>pp1</cppa:PayloadProfileId>
            </cppa:ActionBinding>
            <cppa:ActionBinding id="BII28_002_R" action="AcceptOrder" sendOrReceive="receive" >
                <cppa:ChannelId>ch2</cppa:ChannelId>
                <cppa:PayloadProfileId>pp2</cppa:PayloadProfileId>
            </cppa:ActionBinding>
            <cppa:ActionBinding id="BII28_003_R" action="RejectOrder" sendOrReceive="receive" >
                <cppa:ChannelId>ch2</cppa:ChannelId>
                <cppa:PayloadProfileId>pp2</cppa:PayloadProfileId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
    </cppa:ServiceSpecification>
    

    <cppa:ServiceSpecification>
        <cppa:PartyRole name="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/initiator"/>
        <cppa:CounterPartyRole name="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/responder"/>
        <cppa:ServiceBinding>
            <cppa:Description xml:lang="en">The ebMS3 test service, in initiating mode
                Note that this Service reuses channel "ch1",  so it can be used to test the correct firewall,
                certificate, URL etc. for the SubmitOrder action.
                Here the ebMS3 test service, defined in EBMS3CORE section 5.2.2, is configured to 
                use the same channel as SubmitOrder, so exchange of multiple compressed payload parts 
                are tested both in incoming and outgoing direction with the configured protocol binding.  
            </cppa:Description>
            <cppa:Service>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/service</cppa:Service>
            <cppa:ActionBinding id="test_outgoing" 
                action="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/test" 
                sendOrReceive="send"  >
                <cppa:ChannelId>ch1</cppa:ChannelId>
                <cppa:PayloadProfileId>pp_any_at_least_one</cppa:PayloadProfileId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        <cppa:ServiceBinding>
            <cppa:Description xml:lang="en">The ebCore Agreement Update specification defines a
                CertificateUpdate service. </cppa:Description>
            <cppa:Service>http://docs.oasis-open.org/ebcore/ns/CertificateUpdate/v1.0</cppa:Service>
            <cppa:ActionBinding sendOrReceive="send" action="UpdateCertificate" id="ab_2" >
                <cppa:ChannelId>ch1</cppa:ChannelId>
                <cppa:PayloadProfileId>pp_cur</cppa:PayloadProfileId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="receive" action="ConfirmCertificateUpdate" id="ab_3">
                <cppa:ChannelId>ch2</cppa:ChannelId>
                <cppa:PayloadProfileId>pp_acu</cppa:PayloadProfileId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="receive" action="RejectCertificateUpdate" id="ab_4" >
                <cppa:ChannelId>ch2</cppa:ChannelId>
                <cppa:PayloadProfileId>pp_rcu</cppa:PayloadProfileId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        <cppa:ServiceBinding>
            <cppa:Description xml:lang="en">The ebCore Agreement Update specification defines an
                Agreement Termination service. </cppa:Description>
            <cppa:Service>http://docs.oasis-open.org/ebcore/ns/AgreementTermination/v1.0</cppa:Service>
            <cppa:ActionBinding sendOrReceive="send" action="TerminateAgreement" id="ab_5">
                <cppa:ChannelId>ch1</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="receive" action="ConfirmAgreementTermination" id="ab_6">
                <cppa:ChannelId>ch2</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="receive" action="RejectAgreementTermination" id="ab_7">
                <cppa:ChannelId>ch2</cppa:ChannelId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
    </cppa:ServiceSpecification>
    
    <cppa:ServiceSpecification>
        <cppa:PartyRole name="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/responder"/>
        <cppa:CounterPartyRole name="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/initiator"/>
        <cppa:ServiceBinding>
            <cppa:Description xml:lang="en">The ebMS3 test service,  in responding mode.
                Note that this Service reuses channel "ch5",  so it can be used to test the correct firewall,
                certificate, URL etc. for the AcceptOrder action.
            </cppa:Description>
            <cppa:Service>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/service</cppa:Service>
            <cppa:ActionBinding id="test_incoming" 
                action="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/test" 
                sendOrReceive="receive" >
                <cppa:ChannelId>ch2</cppa:ChannelId>
                <cppa:PayloadProfileId>pp_any_at_least_one</cppa:PayloadProfileId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        <cppa:ServiceBinding>
            <cppa:Description xml:lang="en">Certificate Update in responding mode.</cppa:Description>
            <cppa:Service>http://docs.oasis-open.org/ebcore/ns/CertificateUpdate/v1.0</cppa:Service>
            <cppa:ActionBinding sendOrReceive="receive" action="UpdateCertificate" id="ab_9" >
                <cppa:ChannelId>ch2</cppa:ChannelId>
                <cppa:PayloadProfileId>pp_cur</cppa:PayloadProfileId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="send" action="ConfirmCertificateUpdate" id="ab_10">
                <cppa:ChannelId>ch1</cppa:ChannelId>
                <cppa:PayloadProfileId>pp_acu</cppa:PayloadProfileId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="send" action="RejectCertificateUpdate" id="ab_11" >
                <cppa:ChannelId>ch1</cppa:ChannelId>
                <cppa:PayloadProfileId>pp_rcu</cppa:PayloadProfileId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        <cppa:ServiceBinding>
            <cppa:Description xml:lang="en">Agreement Termination in responding mode.</cppa:Description>
            <cppa:Service>http://docs.oasis-open.org/ebcore/ns/AgreementTermination/v1.0</cppa:Service>
            <cppa:ActionBinding sendOrReceive="receive" action="TerminateAgreement" id="ab_12">
                <cppa:ChannelId>ch2</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="send" action="ConfirmAgreementTermination" id="ab_13">
                <cppa:ChannelId>ch1</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="send" action="RejectAgreementTermination" id="ab_14">
                <cppa:ChannelId>ch1</cppa:ChannelId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
    </cppa:ServiceSpecification>
  
    <cppa:NamedChannel id="ch1" transport="tr1">
        <cppa:Description xml:lang="en">Current protocol,  sending mode</cppa:Description>
        <cppa:ChannelName>community-as4-profile.v1.0</cppa:ChannelName>
        <cppa:SigningCertificateRef certId="p4_signingcert"/>
        <cppa:EncryptionTrustAnchorSetRef certId="trust_anchor_for_p4"/>
    </cppa:NamedChannel>

    <cppa:NamedChannel id="ch2" transport="tr2">
        <cppa:Description xml:lang="en">Current protocol, receiving mode</cppa:Description>
        <cppa:ChannelName>community-as4-profile.v1.0</cppa:ChannelName>
        <cppa:SigningTrustAnchorSetRef certId="trust_anchor_for_p4"/>
        <cppa:EncryptionCertificateRef certId="p4_encryption_cert"/>
    </cppa:NamedChannel>
    
    <cppa:HTTPTransport id="tr1">
        <cppa:Description xml:lang="en">Party P4 in sender/client mode with state-of-the-art security</cppa:Description>
        <cppa:ClientIPv4>127.0.0.1</cppa:ClientIPv4>
        <cppa:TransportLayerSecurity>
            <cppa:TLSProtocol version="1.2">TLS</cppa:TLSProtocol>
            <cppa:CipherSuite>TLS_ECDHE_ECDSA_WITH_AES_128_GCM_SHA256</cppa:CipherSuite>
        </cppa:TransportLayerSecurity>
    </cppa:HTTPTransport>
    
    <cppa:HTTPTransport id="tr2">
        <cppa:Description xml:lang="en">Party P4 in AS4 receiver/server mode</cppa:Description>
        <cppa:Endpoint>https://p4.example.com:8080/someotherhandler</cppa:Endpoint>
        <cppa:TransportLayerSecurity>
            <cppa:TLSProtocol version="1.2">TLS</cppa:TLSProtocol>
            <cppa:CipherSuite>TLS_ECDHE_ECDSA_WITH_AES_128_GCM_SHA256</cppa:CipherSuite>
        </cppa:TransportLayerSecurity>
    </cppa:HTTPTransport>

    <cppa:PayloadProfile id="pp_any_at_least_one">
        <cppa:Description xml:lang="en">Payload agnostic payload profile</cppa:Description>
        <cppa:PayloadPart minOccurs="1" maxOccurs="1">
            <cppa:PartName>businessdocument</cppa:PartName>
        </cppa:PayloadPart>
