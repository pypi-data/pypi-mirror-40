    <cppa:ServiceSpecification>
        <cppa:PartyRole name="Seller"/>
        <cppa:CounterPartyRole name="Buyer"/>
        <cppa:ServiceBinding>
            <cppa:Service>OrderOnly</cppa:Service>
            <cppa:ActionBinding id="BII03_001_R" action="SubmitOrder" sendOrReceive="receive">
                <cppa:ChannelId>ch6</cppa:ChannelId>
                <cppa:ChannelId>ch3</cppa:ChannelId>
                <cppa:PayloadProfileId>pp1</cppa:PayloadProfileId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        <cppa:ServiceBinding>
            <cppa:Service>Ordering</cppa:Service>
            <cppa:ActionBinding id="BII28_001_R" action="SubmitOrder" sendOrReceive="receive">
                <cppa:ChannelId>ch6</cppa:ChannelId>
                <cppa:ChannelId>ch3</cppa:ChannelId>
                <cppa:PayloadProfileId>pp1</cppa:PayloadProfileId>
            </cppa:ActionBinding>
            <cppa:ActionBinding id="BII28_002_S" action="AcceptOrder" sendOrReceive="send">
                <cppa:ChannelId>ch5</cppa:ChannelId>
                <cppa:ChannelId>ch1</cppa:ChannelId>
                <cppa:PayloadProfileId>pp2</cppa:PayloadProfileId>
            </cppa:ActionBinding>
            <cppa:ActionBinding id="BII28_003_S" action="RejectOrder" sendOrReceive="send">
                <cppa:ChannelId>ch5</cppa:ChannelId>
                <cppa:ChannelId>ch1</cppa:ChannelId>
                <cppa:PayloadProfileId>pp2</cppa:PayloadProfileId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
    </cppa:ServiceSpecification>

    <!-- Here the ebMS3 test service, defined in EBMS3CORE section 5.2.2, is configured to 
            use the same channel as SubmitOrder, so exchange of multiple compressed payload parts 
            are tested both in incoming and outgoing direction with the configured protocol binding.  -->


    <cppa:ServiceSpecification>
        <cppa:Description xml:lang="en">The ebMS3 test service, in initiating mode
            Note that this Service reuses channel "ch1",  so it can be used to test the correct firewall,
            certificate, URL etc. for the SubmitOrder action.
        </cppa:Description>
        <cppa:PartyRole
            name="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/initiator"/>
        <cppa:CounterPartyRole
            name="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/responder"/>
        <cppa:ServiceBinding>
            <cppa:Service>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/service</cppa:Service>
            <cppa:ActionBinding id="test_outgoing_a"
                action="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/test"
                sendOrReceive="send" >
                <cppa:ChannelId>ch1</cppa:ChannelId>
                <cppa:PayloadProfileId>pp1</cppa:PayloadProfileId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        <cppa:ServiceBinding>
            <cppa:Service>http://docs.oasis-open.org/ebcore/ns/AgreementUpdate/v1.0</cppa:Service>
            <cppa:ActionBinding sendOrReceive="send" action="UpdateAgreement" id="ab_2">
                <cppa:ChannelId>ch1</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="receive" action="ConfirmAgreementUpdate" id="ab_3">
                <cppa:ChannelId>ch3</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="receive" action="RejectAgreementUpdate" id="ab_4">
                <cppa:ChannelId>ch3</cppa:ChannelId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        <cppa:ServiceBinding>
            <cppa:Service>http://docs.oasis-open.org/ebcore/ns/AgreementTermination/v1.0</cppa:Service>
            <cppa:ActionBinding sendOrReceive="send" action="TerminateAgreement" id="ab_5">
                <cppa:ChannelId>ch1</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="receive" action="ConfirmAgreementTermination" id="ab_6">
                <cppa:ChannelId>ch3</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="receive" action="RejectAgreementTermination" id="ab_7">
                <cppa:ChannelId>ch3</cppa:ChannelId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        
    </cppa:ServiceSpecification>

    <cppa:ServiceSpecification>
        <cppa:Description xml:lang="en">The ebMS3 test service,  in responding mode.
            Note that this Service reuses channel "ch5",  so it can be used to test the correct firewall,
            certificate, URL etc. for the AcceptOrder action.
        </cppa:Description>
        <cppa:PartyRole
            name="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/responder"/>
        <cppa:CounterPartyRole
            name="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/initiator"/>
        <cppa:ServiceBinding>
            <cppa:Service>http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/service</cppa:Service>
            <cppa:ActionBinding id="test_incoming_a"
                action="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/ns/core/200704/test"
                sendOrReceive="receive" >
                <cppa:ChannelId>ch3</cppa:ChannelId>
                <cppa:PayloadProfileId>pp1</cppa:PayloadProfileId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        <cppa:ServiceBinding>
            <cppa:Service>http://docs.oasis-open.org/ebcore/ns/AgreementUpdate/v1.0</cppa:Service>
            <cppa:ActionBinding sendOrReceive="receive" action="UpdateAgreement" id="ab_9">
                <cppa:ChannelId>ch3</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="send" action="ConfirmAgreementUpdate" id="ab_10">
                <cppa:ChannelId>ch1</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="send" action="RejectAgreementUpdate" id="ab_11">
                <cppa:ChannelId>ch1</cppa:ChannelId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
        <cppa:ServiceBinding>
            <cppa:Service>http://docs.oasis-open.org/ebcore/ns/AgreementTermination/v1.0</cppa:Service>
            <cppa:ActionBinding sendOrReceive="receive" action="TerminateAgreement" id="ab_12">
                <cppa:ChannelId>ch3</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="send" action="ConfirmAgreementTermination" id="ab_13">
                <cppa:ChannelId>ch1</cppa:ChannelId>
            </cppa:ActionBinding>
            <cppa:ActionBinding sendOrReceive="send" action="RejectAgreementTermination" id="ab_14">
                <cppa:ChannelId>ch1</cppa:ChannelId>
            </cppa:ActionBinding>
        </cppa:ServiceBinding>
    </cppa:ServiceSpecification>

    <cppa:NamedChannel id="ch1" transport="tr1">
        <cppa:ChannelName>bdx-transport-ebms3-as4-v1r0</cppa:ChannelName>
        <cppa:SigningCertificateRef certId="a_signing_cert"/>
    </cppa:NamedChannel>
    
    <cppa:NamedChannel id="ch3" transport="tr2">
        <cppa:ChannelName>bdx-transport-ebms3-as4-v1r0</cppa:ChannelName>
        <cppa:EncryptionCertificateRef certId="a_encryption_cert"/>
    </cppa:NamedChannel>

    <cppa:NamedChannel id="ch5" transport="tr3">
        <cppa:ChannelName>bdx-transport-ebms3-as2-v1r0</cppa:ChannelName>
        <cppa:SigningCertificateRef certId="a_signing_cert"/>
        <cppa:EncryptionTrustAnchorSetRef certId="trust_anchor_for_a"/>
    </cppa:NamedChannel>
    <cppa:NamedChannel id="ch6" transport="tr4">
        <cppa:ChannelName>bdx-transport-ebms3-as2-v1r0</cppa:ChannelName>
        <cppa:SigningTrustAnchorSetRef certId="trust_anchor_for_a"/>
        <cppa:EncryptionCertificateRef certId="a_encryption_cert"/>
    </cppa:NamedChannel>

    <cppa:HTTPTransport id="tr1">
        <cppa:ClientIPv4>1.2.3.4</cppa:ClientIPv4>
    </cppa:HTTPTransport>
    <cppa:HTTPTransport id="tr2">
        <cppa:Endpoint>https://someserver.de/as4</cppa:Endpoint>
    </cppa:HTTPTransport>

    <cppa:HTTPTransport id="tr3">
        <cppa:ClientIPv4>1.2.3.5</cppa:ClientIPv4>
    </cppa:HTTPTransport>
    <cppa:HTTPTransport id="tr4">
        <cppa:Endpoint>https://someserver.de/as2</cppa:Endpoint>
    </cppa:HTTPTransport>
    
    <cppa:PayloadProfile id="pp1">
        <cppa:Description xml:lang="en">A UBL Order XML document and any number of PDF attachments</cppa:Description>
        <cppa:PayloadPart minOccurs="1" maxOccurs="1">
            <cppa:PartName>businessdocument</cppa:PartName>
            <cppa:MIMEContentType>application/xml</cppa:MIMEContentType>
            <cppa:Schema
                location="http://docs.oasis-open.org/ubl/os-UBL-2.1/xsd/maindoc/UBL-Order-2.1.xsd"/>
        </cppa:PayloadPart>
        <cppa:PayloadPart minOccurs="0" maxOccurs="unbounded">
            <cppa:PartName>attachment</cppa:PartName>
            <cppa:MIMEContentType>application/pdf</cppa:MIMEContentType>
        </cppa:PayloadPart>
    </cppa:PayloadProfile>

    <cppa:PayloadProfile id="pp2">
        <cppa:Description xml:lang="en">A UBL OrderResponse XML document</cppa:Description>
        <cppa:PayloadPart minOccurs="1" maxOccurs="1">
            <cppa:PartName>businessdocument</cppa:PartName>
            <cppa:MIMEContentType>application/xml</cppa:MIMEContentType>
            <cppa:Schema
                location="http://docs.oasis-open.org/ubl/os-UBL-2.1/xsd/maindoc/UBL-OrderResponse-2.1.xsd"
            />
        </cppa:PayloadPart>
    </cppa:PayloadProfile>

