{
	"package": "wasp-c-extensions",
	"homepage": "https://github.com/a1ezzz/wasp-c-extensions",
	"brief_description": "A library with bunch of CPython extensions",
	"author": "Ildar Gafurov",
	"author_email": "dev@binblob.com",
	"maintainer": "Ildar Gafurov",
	"maintainer_email": "dev@binblob.com",
	"numeric_version": "0.0.1",
	"dev_suffix": true,
	"credits": ["Ildar Gafurov"],
	"license": "GNU Lesser General Public License v3",
	"copyright": "Copyright 2018, The Wasp-c-extensions",
	"status": "Development",
	
	"readme_file": "README.md",
	
	"pypi": {
		"keywords": ["wasp", "c-extensions", "threads"],
		"classifiers": [
			"Development Status :: 2 - Pre-Alpha",
			"Intended Audience :: Developers",
			"License :: OSI Approved :: GNU General Public License v3 (GPLv3)",
			"Operating System :: OS Independent",
			"Programming Language :: Python",
			"Programming Language :: Python :: 3.2",
			"Programming Language :: Python :: 3.3",
			"Programming Language :: Python :: 3.4",
			"Programming Language :: Python :: 3.5",
			"Programming Language :: Python :: 3.6",
			"Programming Language :: Python :: 3.7",
			"Programming Language :: Python :: 3 :: Only",
			"Programming Language :: Python :: Implementation :: CPython",
			"Programming Language :: Python :: Implementation :: PyPy",
			"Topic :: Software Development :: Libraries",
			"Topic :: Software Development :: Libraries :: Python Modules"
		]
	},
	"extensions": [
		{
			"module_name": "wasp_c_extensions.threads",
			"sources": [
				"wasp_c_extensions/_threads/module.c",
				"wasp_c_extensions/_threads/atomic.c",
				"wasp_c_extensions/_threads/event.c"
			],
			"depends": [
				"wasp_c_extensions/_threads/atomic.h",
				"wasp_c_extensions/_threads/event.h",
				"wasp_c_extensions/_threads/common.h",
				"wasp_c_extensions/package.json"
			],
			"define_macros": [
				["__PACKAGE_NAME__", "wasp_c_extensions"],
				["__MODULE_NAME__", "threads"],
				["__ATOMIC_COUNTER_NAME__", "WAtomicCounter"],
				["__PTHREAD_EVENT_NAME__", "WPThreadEvent"],
				["__DEFAULT_SIGNALS_POLLING_TIMEOUT__", "10"]
			]
		}
	]
}
